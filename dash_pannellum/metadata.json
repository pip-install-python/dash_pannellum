{
  "src/lib/components/DashPannellum.react.js": {
    "description": "DashPannellum is a component for displaying panoramic images and videos.\nIt supports various modes including tours, multi-resolution images, and 360\u00b0 videos.",
    "displayName": "DashPannellum",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID used to identify this component in Dash callbacks."
      },
      "width": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The width of the panorama viewer.",
        "defaultValue": {
          "value": "'600px'",
          "computed": false
        }
      },
      "height": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The height of the panorama viewer.",
        "defaultValue": {
          "value": "'400px'",
          "computed": false
        }
      },
      "tour": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "Configuration object for the tour mode."
      },
      "multiRes": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "Configuration object for multi-resolution panoramas."
      },
      "video": {
        "type": {
          "name": "shape",
          "value": {
            "sources": {
              "name": "arrayOf",
              "value": {
                "name": "shape",
                "value": {
                  "src": {
                    "name": "string",
                    "required": true
                  },
                  "type": {
                    "name": "string",
                    "required": true
                  }
                }
              },
              "required": true
            },
            "poster": {
              "name": "string",
              "required": false
            }
          }
        },
        "required": false,
        "description": "Configuration object for video panoramas."
      },
      "customControls": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "If true, enables custom controls for the panorama viewer.",
        "defaultValue": {
          "value": "false",
          "computed": false
        }
      },
      "showCenterDot": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "If true, displays a center dot in the panorama viewer.",
        "defaultValue": {
          "value": "false",
          "computed": false
        }
      },
      "setProps": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Dash-assigned callback that should be called to report property changes\nto Dash, to make them available for callbacks."
      },
      "autoLoad": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "If true, automatically loads the panorama without user interaction.",
        "defaultValue": {
          "value": "false",
          "computed": false
        }
      },
      "compass": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "If true, displays a compass in the panorama viewer.",
        "defaultValue": {
          "value": "false",
          "computed": false
        }
      },
      "northOffset": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "The offset, in degrees, of the center of the panorama from North.",
        "defaultValue": {
          "value": "0",
          "computed": false
        }
      },
      "pitch": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "The current pitch of the panorama view."
      },
      "yaw": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "The current yaw of the panorama view."
      },
      "currentScene": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID of the current scene in tour mode."
      }
    }
  }
}